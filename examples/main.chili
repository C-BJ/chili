use std.{
    c.printf,
    fmt.println,
    // types
}

let A = struct {
    a: int,
    b: int
}

let main = fn() {
    let a: A = .{ a: 1, b: 2 };
    printf("%d %d\n".data, a.a, a.b);
    let arr = .[1, 2, 3];
    print_array(arr);
}

let print_array = fn(a: [3]int) {
    for a {
        printf("a[%d] = %d\n".data, it_index, it)
    }
}

@run(main())