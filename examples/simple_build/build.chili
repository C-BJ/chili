let ? = @import("std/build");

let build = fn() {
	let build_options = BuildOptions {
        input_file: "src/main.chili",
        output_file: "out/main",
        target: BUILD_TARGET_AUTO,
        release: false,
        run_when_done: false,
	};

	let workspace = Workspace {
		name: "hello",
		build_options
	};

	@workspace_start(workspace);
};

@run(build());